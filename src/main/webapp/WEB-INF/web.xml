<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns=""
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation=""
         version="4.0">

  <display-name>Archetype Created Web Application</display-name>

  <filter>
    <filter-name>BinarySearchFilter</filter-name>
    <filter-class>com.step.hryshkin.BinarySearchFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>BinarySearchFilter</filter-name>
    <url-pattern>/searching_page.jsp</url-pattern>
  </filter-mapping>

  <error-page>
    <error-code>404</error-code>
    <location>/404.jsp</location>
  </error-page>

  <welcome-file-list>
    <welcome-file>homepage.jsp</welcome-file>
  </welcome-file-list>
</web-app>
